@model IEnumerable<ImfuyoRanch.Models.Equipment_Hire.EquipmentType>

@{
    ViewBag.Title = "Index";
}

<h2>Equioment Type</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table table-striped">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.EquipementTypeName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.EquipementTypeName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.EquipmentTypeId }) |
            @Html.ActionLink("Details", "Details", new { id=item.EquipmentTypeId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.EquipmentTypeId })
        </td>
    </tr>
}

</table>
